<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipselabs.rcputils.snippets.application.SnippetsApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipselabs.rcputils.snippets.SnippetsPerspectiveFactory"
            id="org.eclipselabs.rcputils.snippets"
            name="RCP Snippets">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="org.eclipselabs.rcputils.snippets.snippet01tableviewerbuilder.Snippet01TableViewerBuilder"
            id="org.eclipselabs.rcputils.snippets.Snippet01TableViewerBuilder"
            name="01 TableViewerBuilder"
            restorable="true">
      </view>
      <view
            class="org.eclipselabs.rcputils.snippets.snippet02stocks.Snippet02StocksViewPart"
            id="org.eclipselabs.rcputils.snippets.Snippet02Stocks"
            name="02 Stocks"
            restorable="true">
      </view>
      <view
            class="org.eclipselabs.rcputils.snippets.snippet03concurrent.Snippet03Concurrent"
            id="org.eclipselabs.rcputils.snippets.Snippet03Concurrent"
            name="03 Concurrent"
            restorable="true">
      </view>
   </extension>

</plugin>
